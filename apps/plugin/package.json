{
  "name": "templetto-figma-plugin",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "description": "Templetto is a plugin for Figma that generates pdfs from templates",
  "scripts": {
    "dev": "ENV=DEV rollup -c --watch",
    "build": "ENV=PRODUCTION rollup -c && rm dist/ui.js",
    "check": "svelte-check --tsconfig ./tsconfig.app.json && tsc -p tsconfig.backend.json"
  },
  "devDependencies": {
    "@figma/plugin-typings": "^1.116.0",
    "@rollup/plugin-babel": "^6.0.4",
    "@rollup/plugin-commonjs": "^28.0.6",
    "@rollup/plugin-dsv": "^3.0.5",
    "@rollup/plugin-html": "^2.0.0",
    "@rollup/plugin-json": "^6.1.0",
    "@rollup/plugin-node-resolve": "^16.0.1",
    "@rollup/plugin-terser": "^0.4.4",
    "@rollup/plugin-typescript": "^12.1.4",
    "@tsconfig/svelte": "^5.0.4",
    "@types/hyperscript": "^0.0.7",
    "cssnano": "^7.1.0",
    "rollup": "^4.46.2",
    "rollup-plugin-livereload": "^2.0.5",
    "rollup-plugin-postcss": "^4.0.2",
    "rollup-plugin-svelte": "^7.2.2",
    "rollup-plugin-svg": "^2.0.0",
    "svelte": "^5.28.1",
    "svelte-check": "^4.1.6",
    "svelte-preprocess": "^6.0.3",
    "typescript": "~5.8.3",
    "vite-plugin-singlefile": "^2.3.0"
  },
  "dependencies": {
    "hyperscript": "^2.0.2",
    "tslib": "^2.8.1"
  }
}
